<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.greatlarder.technicalassistant.controller.fragment_item.ItemIpAddress">
   <top>
      <VBox alignment="TOP_RIGHT" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="imgExit" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#exit" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/closed.png" />
               </image>
            </ImageView>
            <HBox fx:id="hBox" alignment="TOP_CENTER" spacing="5.0">
               <children>
                  <TextField fx:id="net1" onKeyReleased="#onKeyNet1" prefWidth="40.0" />
                  <Label text="." />
                  <TextField fx:id="net2" onKeyReleased="#onKeyNet2" prefWidth="40.0" />
                  <Label text="." />
                  <TextField fx:id="subNet" editable="false" prefWidth="40.0" text="1" />
                  <Label text="." />
                  <TextField fx:id="device" editable="false" prefWidth="40.0" />
                  <Button fx:id="btnStart" mnemonicParsing="false" onMouseClicked="#start" text="Start" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </top>
</BorderPane>
