                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/home.png" />
                     </image>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin></Label>
                <Label fx:id="labelManuals" maxWidth="1.7976931348623157E308" onMouseClicked="#clickDocumentation" styleClass="vBox_button" stylesheets="@../../../css/main.css" text="Документация" textAlignment="CENTER">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/documents.png" />
                     </image>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></Label>
                <Label fx:id="labelSettings" maxWidth="1.7976931348623157E308" onMouseClicked="#clickPageSettings" styleClass="vBox_button" stylesheets="@../../../css/main.css" text="Настройки" textAlignment="CENTER">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/settings.png" />
                     </image>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></Label>
                     <Label fx:id="labelInfo" maxWidth="1.7976931348623157E308" onMouseClicked="#openPageInfo" styleClass="vBox_button" stylesheets="@../../../css/main.css" text="Информация">
               <graphic>
                       <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <Image url="@../../../images/info.png" />
                       </ImageView>
               </graphic>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Label>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            </VBox>
        </SplitPane>
    </left>
    <opaqueInsets>
        <Insets />
