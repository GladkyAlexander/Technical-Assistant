<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vBox" styleClass="item_equipment" stylesheets="@../../css/main.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.greatlarder.technicalassistant.controller.fragment_item.ItemEquipmentController">
   <children>
      <HBox spacing="3.0">
         <children>
             <ImageView fx:id="ivPhoto" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                 <Image url="@../../images/all_equipment.png" />
             </ImageView>
            <VBox>
               <children>
                   <Label fx:id="serialNumber" maxWidth="1.7976931348623157E308" />
                   <Label fx:id="model" maxWidth="1.7976931348623157E308" />
                   <Label fx:id="date" maxWidth="1.7976931348623157E308" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
